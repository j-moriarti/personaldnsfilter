<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<!-- outer rectangle -->
	<item>
		<shape android:shape="rectangle">
			<corners android:radius="4dp" />
			<size
				android:width="22dp"
				android:height="22dp"/>
			<stroke
				android:width="2dp"
				android:color="#fafafa">
			</stroke>
			<!-- fake background -->
			<solid
				android:color="#00FFFFFF">
			</solid>
		</shape>
	</item>

</layer-list>